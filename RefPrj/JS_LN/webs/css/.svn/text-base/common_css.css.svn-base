/** 主页基本样式设置 */
body {
	color: 				#004DA1;
	font-size:			14px;
	font-family:		Geneva, Arial, Helvetica, sans-serif;
	text-decoration: 	none;
	text-align:center;
}

/* 机框槽定义 */	
td.slot{
	width: 10px;
}

/*机框单板正常状态定义*/		
td.board{
	font: "Times New Roman";
	font-size:24px;
	text-align:center;
	background-color:#ADD8E6;
}

/*机框单板禁用状态定义*/		
td.fixedBoard{
	font: "Times New Roman";
	font-size:24px;
	text-align:center;
	background-color:#ADD8E6;
	cursor: pointer;
}

/** 通用DIV层样式设置,百分之百宽度设置 */
.divStyle {	 
	display:block;
	padding-right: 5px; 
	padding-left:  5px;
	padding-bottom: 5px; 
	overflow:     hidden; 
	width: 100%; 
	padding-top: 5px;
}

/** 通用DIV层样式设置，百分之八十宽度设置 */
.divStyle80 {	 
	display:block;
	padding-right: 5px; 
	padding-left:  5px;
	padding-bottom: 5px; 
	overflow:     hidden; 
	width: 80%; 
	padding-top: 5px;
	margin:auto;
}

/*通用下拉菜单设置-----*/
.select{
	width:100%;
	font-size:14px;
	height:18px;
}

/** 通用按钮的设置----- */
.button{
	width: 				80px;
	height: 			18px;
	font-size: 			12px;
	font-weight:		bold;
	background-image: url(../images/button.gif);
	background-repeat: no-repeat;
	background-position: center center;
	background-color: #1874CD;
	border-width: 0px;
	border-style: none;
	margin: 0px;
	padding: 0px;
	text-indent: 0px;
	cursor:pointer;
}

/*通用文本框设置*/
.text{
	width:100%;
	height:20px;
	padding:0px 0px 0px 0px;
	margin:0px 0px 0px 0px;
	line-height:normal;
	border:0px solid #ffffff;
}

/*通用body体背景设置*/
body.banner {
	font-size: 			11PX; 
	margin: 			0PX; 
	font-style:			normal; 
	background-color:#FFFFFF;
	width: 100%;
	
}

/** 通用表格的外层边框设置 ,简单无背景设置*/
table.tableStyleSimple{
	width: 				100%;
	margin:				0;
	border:				1px; 
}


/** 通用表格的外层边框设置,表底色为蓝 */
table.tableStyle{
	width: 				100%;
    table-layout: fixed;
	/*word-break:break-all;*/
	word-wrap:   break-word;
	margin:				0;
	border:				1px solid #104E8B; 
	background-color:#1874CD;
}

/** 通用表格的外层边框设置 ，表底色为白*/
table.tableStyleWhite{
	width: 				100%;
    table-layout: fixed;
	word-wrap:break-word;
	margin:				0;
	border:				1px solid #104E8B; 
	background-color:#FFFFFF;
}

/** 通用表格的外层边框设置 ,简单无背景设置,80%宽度*/
