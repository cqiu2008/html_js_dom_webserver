/* 消息窗口 */
.msg-window{
	background:#fff;overflow:hidden;
	font-size:12px;	text-align:left;
}

/* 窗口标题 */
.msg-titlel{padding-left:3px;background:#CFD7EC url(../images/title_bg_left.gif) no-repeat 0 0}
.msg-titler{padding-right:3px;background:#CFD7EC url(../images/title_bg_right.gif) no-repeat right 0;}
.msg-titlec{background:#CFD7EC url(../images/title_bg_center.gif) repeat-x 0 0;overflow:hidden;height:25px;line-height:25px;CURSOR: move}
.msg-title{font-size:12px;font-weight:bold;color:#fff;margin-left:5px;float:left}
.msg-tools{float:right;margin-top:5px}
.msg-tools strong{display:none}

/* 窗口体 */
.msg-bodyl{background:url(../images/win_l.gif) repeat-y 0 0;padding-left:3px;}
.msg-bodyr{background:url(../images/win_r.gif) repeat-y right 0;padding-right:3px;}
.msg-bodyc{background:url(../images/content_bg.gif) repeat-x 0 0;padding:0;overflow:auto;padding:0;font-size:12px;}

/* 按钮 */
.msg-btn{text-align:center}

/*窗口尾*/
.msg-footl{background:url(../images/win_lb.gif) no-repeat 0 bottom;padding-left:3px}
.msg-footr{background:url(../images/win_rb.gif) no-repeat right bottom;padding-right:3px}
.msg-footc{background:url(../images/win_b.gif) repeat-x 0 bottom;height:3px;font-size:3px}

/* 窗口提示图标 */
.msg_alert{
	padding-left:90px;
	background:url(../images/info.gif) no-repeat 20px 50%
}
.msg_succeed{
	padding-left:90px;
	background:url(../images/right.gif) no-repeat 20px 50%
}
.msg_error{
	padding-left:90px;
	background:url(../images/err.gif) no-repeat 20px 50%
}
.msg_confirm{
	padding-left:90px;
	background:url(../images/ask.gif) no-repeat 20px 50%
}
.msg_alert .msg-content,.msg_succeed .msg-content,.msg_error .msg-content,.msg_confirm .msg-content{padding:50px 0 0}
/*图标公共定义*/
.msg-tools div{
	cursor:pointer;
	width:16px;height:16px;float:left;margin:0 1px;
	background:url(../images/ico.gif) no-repeat 0 0;
}
.msg_close{
	background-position:-16px 0 !important;
}
.msg_max{
	background-position:0 -16px !important;
}
.msg_min{
	background-position:0 0 !important;
}
.msg_normal{
	background-position:-16px -16px !important;
}

